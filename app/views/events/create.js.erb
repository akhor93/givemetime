$('#event_container').append("<%= j render(:partial => 'events/event', :locals => {event: @event}) %>").trigger('append_event', [ '<%= @event["summary"] %>', '<%= @event["end"]["dateTime"] - @event["start"]["dateTime"] %>', '<%= @event["start"]["dateTime"] %>'] );

<% if source == :todo %>
	//remove corresponding todo from list
	$('#todo_<%= id %>').remove();
	//decrement badge count
	decrementBadge('todo_count_badge');
	//TODO: if count is 0 now then we can collapse the panel

	//Remove from table
	$('#todo_row_' + '<%= id  %>').remove();
	updateRowIndicies('todos_table');
<% elsif source == :activity %>
	//do nothing
<% elsif source == :event %>
	//do nothing
<% end %>
